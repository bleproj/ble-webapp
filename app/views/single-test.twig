{% include 'parts/header.twig' %}
<div class="container-fluid">
    <div class="row-fluid">
        <div class="span3">
            <!--Sidebar content-->
            <h2>{{ test.name }}</h2>

            <p><strong>Test type:</strong> {{ test.type }} <br>
                <strong>Description: </strong> {{ test.description }}<br>
                <strong>Ping count: </strong> {{ test.testData().pingcount }}<br>
                <strong>Host: </strong> {{ test.testData().host }}<br>
                <strong>Average ping time: </strong> {{ averagepingtime }}
            </p>
            <hr>
        </div>
        <div class="span9">
            <!--Body content-->
            <div id="container"></div>
            <h2>Events</h2>
            <table class="table table-striped">
                <thead>
                <tr>
                    <th>eventID</th>
                    <th>eventType</th>
                    <th>timeStamp</th>
                    <th>ms</th>
                </tr>
                </thead>
                <tbody>
                {% for event in events %}
                    <tr>
                        <td>{{ event.id }}</td>
                        <td>{{ event.type }}</td>
                        <td>{{ event.timestamp }}</td>
                        <td>{{ event.eventdata().ms }}</td>
                    </tr>
                {% endfor %}
                </tbody>
            </table>
        </div>
    </div>
</div>
<script type="text/javascript">
    testData = [{% for event in events %}{{ event.eventdata().ms }},{% endfor %}];
</script>
{% include 'parts/footer.twig' %}